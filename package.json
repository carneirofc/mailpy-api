  "dependencies": {
    "@azure/msal-node": "^1.0.0-beta.3",
    "@microsoft/microsoft-graph-client": "^2.2.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
