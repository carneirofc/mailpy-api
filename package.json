{
  "name": "mailpy-api",
  "version": "1.0.0",
  "description": "Rest API for Mailpy",
  "author": "Claudio Carneiro",
  "license": "GPL-3.0",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint && npm run copy-static-assets",
    "build-db": "ts-node initMongoDB.ts",
    "build-ts": "tsc",
    "clean": "rm -rf build/",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "dev": "nodemon --inspect --exec babel-node src/index.js",
    "lint": "tsc --noEmit",
    "start": "node build/src/index.js",
    "test": "jest --verbose --detectOpenHandles src",
    "test-watch": "jest --verbose --detectOpenHandles --onlyChanged --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carneirofc/mailpy-api.git"
  },
  "bugs": {
    "url": "https://github.com/carneirofc/mailpy-api/issues"
  },
  "homepage": "https://github.com/carneirofc/mailpy-api#readme",
  "dependencies": {
    "@azure/msal-node": "^1.0.0-beta.3",
    "@microsoft/microsoft-graph-client": "^2.2.1",
    "@types/mongodb": "^3.6.18",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.2",
    "mongodb": "^3.6.3",
    "monk": "^7.3.2",
    "morgan": "^1.10.0",
    "nodaemon": "0.0.5",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/node-fetch": "^2.5.10",
    "@types/passport": "^1.0.6",
    "@types/passport-azure-ad": "^4.3.0",
    "@types/shelljs": "^0.8.8",
    "eslint": "^7.29.0",
    "faker": "^5.1.0",
    "jest": "^27.0.5",
    "jest-environment-node": "^26.6.2",
    "mongodb-memory-server": "^6.9.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.3.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
